    "require": {
        "aws/aws-sdk-php": "3.0.*@dev",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bao Pham",
            "email": "gbaopham@gmail.com"
        }
    ],
